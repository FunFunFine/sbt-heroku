# Stage the distribution and ensure it can be deployed.
> createApp
> show stage
$ exists target/universal/stage/bin/scala-getting-started
> show deployHeroku
$ exists target/heroku/app/
$ exists target/heroku/app/.jdk/bin/java
$ exists target/heroku/app/.profile.d/certs.sh
$ exists target/heroku/jdk-pkg.tar.gz
$ exists target/heroku/slug.tgz
$ exists target/heroku/diagnostics.log
> check
> cleanup